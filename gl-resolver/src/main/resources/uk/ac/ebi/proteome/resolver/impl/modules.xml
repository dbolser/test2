<?xml version="1.0" encoding="UTF-8"?>
<tasks>
	<task task="TRNASCAN">
		<!-- subject is either the component subject or some other controlled term -->
		<concern concern="EMBLSeq">
			<moduleSpecification name="seqMat"
				className="impl.SequenceFileMaterializer">
				<!-- parameters are one way to use the same class in different ways -->
				<properties>
					<property key="output">FASTA</property>
				</properties>
			</moduleSpecification>
		</concern>
		<concern concern="SGDSeq">
			<moduleSpecification name="seqMat"
				className="impl.SequenceFileMaterializer">
				<!-- parameters are one way to use the same class in different ways -->
				<properties>
					<property key="output">FASTA</property>
				</properties>
			</moduleSpecification>
		</concern>
		<concern concern="TAIRSeq">
			<moduleSpecification name="seqMat"
				className="impl.SequenceFileMaterializer">
				<!-- parameters are one way to use the same class in different ways -->
				<properties>
					<property key="output">FASTA</property>
				</properties>
			</moduleSpecification>
		</concern>
		<concern concern="identification">
			<!-- concern here is specific to identifiying new components -->
			<moduleSpecification concern="identification" name="seqId"
				className="impl.SequenceFileMaterializer">
				<properties>
					<property key="sources">EMBL,TAIR,SGD</property>
				</properties>
			</moduleSpecification>
		</concern>
	</task>
</tasks>
