<?xml version='1.0' encoding='UTF-8'?>

<!DOCTYPE dataset [
	<!ELEMENT dataset (
		OLD_UNSEC*,
		UNSEC*,
		SECTION_ARCHIVE*
	)>
	
	<!ELEMENT OLD_UNSEC EMPTY>
	<!ATTLIST OLD_UNSEC
		ID CDATA #REQUIRED
	>
	
	<!ELEMENT UNSEC EMPTY>
	<!ATTLIST UNSEC
		ID CDATA #REQUIRED
		SECTIONID CDATA #IMPLIED
	>
	
	<!ELEMENT SECTION_ARCHIVE EMPTY>
	<!ATTLIST SECTION_ARCHIVE
		ID CDATA #REQUIRED
		SECTION_ID CDATA #REQUIRED
		EXTERNAL_VERSION CDATA #IMPLIED
		VERSION_ID CDATA #REQUIRED
		CREATED CDATA #REQUIRED
		DELETED CDATA #IMPLIED
		ROWCOUNT CDATA #REQUIRED
	>
]>

<dataset>

	<OLD_UNSEC ID="1"/>
	<OLD_UNSEC ID="2"/>
	
	<UNSEC ID="1" SECTIONID="1"/>
