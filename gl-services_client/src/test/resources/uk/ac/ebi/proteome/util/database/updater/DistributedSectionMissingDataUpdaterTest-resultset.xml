<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dataset [
	<!ELEMENT dataset (
		ONE*,
		TWO*,
		TMP_ONE*,
		TMP_TWO*,
		CV_TABLE*,
		CV_SECTION*,
		SECTION_ARCHIVE*,
		PROCESS_OWNER*,
		UPDATE_PROCESS_OWNER*
	)>

	<!ELEMENT ONE EMPTY>
	<!ATTLIST ONE
		ID CDATA #REQUIRED
		VAL CDATA #REQUIRED
		SECTIONID CDATA #REQUIRED
	>

	<!ELEMENT TWO EMPTY>
	<!ATTLIST TWO
		ID CDATA #REQUIRED
		VAL CDATA #REQUIRED
		SECTIONID CDATA #REQUIRED
	>

	<!ELEMENT TMP_ONE EMPTY>
	<!ATTLIST TMP_ONE
		ID CDATA #REQUIRED
		VAL CDATA #REQUIRED
		SECTIONID CDATA #REQUIRED
	>

	<!ELEMENT TMP_TWO EMPTY>
	<!ATTLIST TMP_TWO
		ID CDATA #REQUIRED
		VAL CDATA #REQUIRED
		SECTIONID CDATA #REQUIRED
	>

	<!ELEMENT CV_TABLE EMPTY>
	<!ATTLIST CV_TABLE
		ID CDATA #REQUIRED
		NAME CDATA #REQUIRED
		SKIP_SHRINKAGE_CHECK CDATA #REQUIRED
	>

	<!ELEMENT CV_SECTION EMPTY>
	<!ATTLIST CV_SECTION
		ID CDATA #REQUIRED
		TABLE_ID CDATA #REQUIRED
		NAME CDATA #REQUIRED
		SKIP_SHRINKAGE_CHECK CDATA #REQUIRED
	>

	<!ELEMENT PROCESS_OWNER EMPTY>
	<!ATTLIST PROCESS_OWNER
		ID CDATA #REQUIRED
		NAME CDATA #REQUIRED
		DESCRIPTION CDATA #IMPLIED
		CONTACT CDATA #IMPLIED
	>

	<!ELEMENT UPDATE_PROCESS_OWNER EMPTY>
	<!ATTLIST UPDATE_PROCESS_OWNER
		ID CDATA #REQUIRED
		PARENTID CDATA #IMPLIED
		NAME CDATA #REQUIRED
		DESCRIPTION CDATA #IMPLIED
	>

	<!ELEMENT SECTION_ARCHIVE EMPTY>
	<!ATTLIST SECTION_ARCHIVE
		ID CDATA #REQUIRED
		SECTION_ID CDATA #REQUIRED
		EXTERNAL_VERSION CDATA #IMPLIED
		VERSION_ID CDATA #REQUIRED
		CREATED CDATA #REQUIRED
		DELETED CDATA #IMPLIED
		ROWCOUNT CDATA #REQUIRED
	>
]>

<dataset>

	<ONE ID="1" VAL="A" SECTIONID="1"/>
	<ONE ID="2" VAL="B" SECTIONID="1"/>

	<TWO ID="1" VAL="X" SECTIONID="1"/>
	<TWO ID="3" VAL="Z" SECTIONID="2"/>

	<CV_TABLE ID="1" NAME="TAB" SKIP_SHRINKAGE_CHECK="0"/>

	<CV_SECTION ID="1" TABLE_ID="1" NAME="A" SKIP_SHRINKAGE_CHECK="0"/>
	<CV_SECTION ID="2" TABLE_ID="1" NAME="B" SKIP_SHRINKAGE_CHECK="0"/>

	<SECTION_ARCHIVE ID="1" SECTION_ID="1" VERSION_ID="1" CREATED="-1" ROWCOUNT="2"/>
	<SECTION_ARCHIVE ID="2" SECTION_ID="2" VERSION_ID="1" CREATED="1" ROWCOUNT="0"/>

</dataset>